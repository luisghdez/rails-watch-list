<div class="row">
  <% @lists.each do |l| %>
  <div class="col-sm-6">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%= l.name %></h5>
        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
        <%= link_to 'go', list_path(l) %>
      </div>
    </div>
  </div>
  <% end %>
</div>
